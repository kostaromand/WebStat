<Window x:Class="WebStat.MainWindow"
        xmlns:dxtm="http://schemas.devexpress.com/winfx/2008/xaml/treemap" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebStat"
        mc:Ignorable="d"
        Title="WebStat" Height="850" Width="1500">
    <DockPanel>
        <Grid MaxHeight="200" DockPanel.Dock="Top">
            <Button Width="200" Height="50" DockPanel.Dock="Top" Name="getPath" Click="getPath_Click">
                Путь
            </Button>
        </Grid>
        <DockPanel Name="WebStat" Margin="5,5,5,5">
            <TextBlock Name="WebStatTitle" Background="#700000"  DockPanel.Dock="Top" TextAlignment="Center" Foreground="White" FontSize="18">
                WebStat
            </TextBlock>
            <Canvas Background="#990000" Name="WebStatCanvas">
            </Canvas>
        </DockPanel>
    </DockPanel>
</Window>
